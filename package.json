{
    "name": "phaser3-typescript-rollup-ghpages-template",
    "version": "1.0.0",
    "description": "A Phaser 3 Project Template using Rollup and TypeScript and GH pages to deploy",
    "main": "src/game.ts",
    "scripts": {
        "dev": "rollup --config rollup.config.dev.mjs",
        "build": "rollup --config rollup.config.dist.mjs",
        "watch": "rollup --watch --config rollup.config.dev.mjs",
        "deploy": "npm run build && gh-pages -d dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gfxblit/phaser3-typescript-rollup-ghpages-template"
    },
    "author": "Bill Chen (https://github.com/settings/profile.gfxblit)",
    "license": "MIT",
    "homepage": "https://github.com/gfxblit/phaser3-typescript-rollup-ghpages-template",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.1.0",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "eslint": "^8.38.0",
        "gh-pages": "^4.x",
        "phaser": "^3.60.0",
        "rollup": "^3.20.2",
        "rollup-plugin-serve": "^2.0.2",
        "typescript": "^5.0.4"
    }
}
